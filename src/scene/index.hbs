<a-scene id="aframe-project" auto-enter-vr antialias="false">
  <!-- assets + templates -->
  <a-assets id="assets">
    <audio id="click-sound" crossorigin="anonymous" src="https://cdn.aframe.io/360-image-gallery-boilerplate/audio/click.ogg"></audio>
  </a-assets>

  <!-- camera + cursor -->
  <a-camera mouse-cursor wasd-controls-enabled="false" position="0 -1.5 0"></a-camera>

  <!-- layout -->
  <a-entity grid="content: {{contentStr}}"></a-entity>
</a-scene>
